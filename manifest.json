{
	"name": "Manoel Gomes Replacer",
	"description": "Replace all images with Manoel Gomes.",
	"author": "Daviuee",
	"manifest_version": 2,
	"version": "1.0.0",
	"browser_action": {
		"default_title": "Manoel Gomes Replacer",
		"default_popup": "index.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"run_at": "document_end",
			"js": ["scripts/Main.js"],
			"all_frames": true
		}
	],
	"homepage_url": "https://github.com/daviue/manoelgomes-replacer",
	"icons": {
		"128": "icon128.png"
	},
	"permissions": ["storage"],
	"short_name": "Manoel Gomes Replacer"
}